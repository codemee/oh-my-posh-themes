{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#7E46B6",
          "properties": {
            "display_host": false,
            "template": "{{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }} "
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "template": "in "
          },
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#87FF00",
          "properties": {
            "style": "full",
            "template": "{{ .Path }} "
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#5FD7FF",
          "properties": {
            "branch_ahead_icon": "",
            "branch_behind_icon": "",
            "branch_gone_icon": "",
            "branch_icon": "",
            "branch_identical_icon": "",
            "cherry_pick_icon": "",
            "commit_icon": "",
            "fetch_stash_count": false,
            "fetch_status": true,
            "merge_icon": "",
            "rebase_icon": "",
            "revert_icon": "",
            "tag_icon": "",
            "template": "<#ffffff>on</> {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \u25cf<#D75F00>{{ .Working.String }}</>{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }}{{ end }}{{ if .Staging.Changed }} \u25cf<#87FF00>{{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} "
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#D75F00",
          "properties": {
            "template": "Î» "
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 1
}
